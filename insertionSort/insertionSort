def insertionSort(tab):
    for i in range(1, len(tab)):    
        klucz=tab[i]

        j=i-1

        while j>=0 and klucz< tab[j]:
                        tab[j+1]=tab[j]
                        j-=1
                        tab[j+1]=klucz
tab=[99,12,5,3,2,1]
print("nieposortowane:")
print(tab)


print("posortowane:")
insertionSort(tab)
print(tab)
